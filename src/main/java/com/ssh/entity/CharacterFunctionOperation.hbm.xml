<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.entity.CharacterFunctionOperation" table="character_function_operation" schema="mysys">
        <id name="id">
            <column name="id" sql-type="int(20)"/>
        </id>
        <property name="characterid">
            <column name="characterid" sql-type="int(20)"/>
        </property>
        <property name="functionid">
            <column name="functionid" sql-type="int(20)" not-null="true"/>
        </property>
        <property name="operationid">
            <column name="operationid" sql-type="int(20)"/>
        </property>
    </class>
</hibernate-mapping>